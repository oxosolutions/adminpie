{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "intervention/image": "^2.4",
        "khill/lavacharts": "3.0.*",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2.0",
        "maatwebsite/excel": "^2.1",
        "spatie/laravel-fractal": "^4.3",
        "yajra/laravel-datatables-buttons": "^2.0",
        "yajra/laravel-datatables-html": "^2.0",
        "yajra/laravel-datatables-oracle": "^7.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "dev-master as 1.1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "PHPMySQLWrapper\\MySQLWrapper\\" : "packages/phpmysqlwrapper/mysqlwrapper/src"
        },
        "files": ["app/Helpers/widget_function.php", "app/Helpers/global_helper.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
